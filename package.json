{
  "name": "osmedeus-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bunx --bun next dev",
    "dev:clean": "rm -rf .next .next-static && bunx --bun next dev",
    "build": "bunx --bun next build",
    "build:dist": "rm -rf dist .next-static && NEXT_STATIC_EXPORT=true bunx --bun next build && mv .next-static dist",
    "start": "bunx --bun next start",
    "lint": "bunx --bun eslint . --max-warnings=0"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "next": "16.1.1",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.17.0",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2"
  }
}
